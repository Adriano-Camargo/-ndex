// Cálculo de IMC
console.log("Cálculo de IMC")
const aluno = 'Carlos'
const peso = 84
const altura = 1.88

imc = peso / (altura * altura)

if(imc >= 30) {
    console.log(`${aluno}, você está acima do peso.`)
} else {
    console.log(`${aluno}, você não está acima do peso.`)
}
